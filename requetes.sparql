#Requêtes vérifiées par fuseki sur serveur local version 3.4.0

#nom et date de début des projets commencés avant 2009, rangé par date de début
SELECT ?titre ?date_deb_projet
WHERE { ?x rdf:type doap:Project;
           doap:name ?titre;
           time:hasBeginning ?date_deb_projet.
  FILTER(?date_deb_projet < "2009").}
ORDER BY ?date_deb_projet


#listes des pays partenaires des projets les plus couteux
Select distinct ?pays
Where { ?x dbo:Location ?pays;
           rdf:type doap:Project;
  			frapo:budgetedAmount ?argent.
}
Order By ?argent
Limit 10
